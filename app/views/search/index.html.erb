<h1><%=@members.count%> <%= @nation %> Members </h1>


  <% @members.each do |member| %>
    <ul class= "member">
      <h3>Name: <%= member[:name]%></h3>
      <p>Photo: <img src=<%= member[:photoUrl] if !nil %>></p>
      <p>Affiliation: <%= member[:affiliation]%></p>
      <p>Enemies: <%= member[:enemies]%></p>
      <p>Allies: <% member[:allies]%></p>
    </ul>
  <% end %>
